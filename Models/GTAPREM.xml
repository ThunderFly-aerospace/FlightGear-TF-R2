<PropertyList>
<nasal>
  <load>
    <!--
      ##############################################################################
      # The on-load Nasal is not executed when this file is loaded as the user
      # aircraft. The code is only executed if the file is loaded for a remote
      # aircraft in Multi-Player mode (MP)
      ##############################################################################
    -->
    <![CDATA[
      var livery_update = aircraft.livery_update.new("Aircraft/GTAVPREM/Models/Liveries");
    ]]>
    </load>
  
  <unload><![CDATA[
    livery_update.stop();
  ]]></unload>
</nasal>
<path>GTAPREM.ac</path>
<!-- ========== Effects ========= -->
<animation>
  <type>material</type>
  <object-name>GTAPREM-Body</object-name>
  <object-name>D1</object-name>
  <property-base>sim/model/livery</property-base>
  <texture-prop>texture</texture-prop>
  <texture>Liveries/BaseColor.png</texture>
</animation>
<effect>
 <inherits-from>Effects/glass</inherits-from>
 <object-name>Windows</object-name>
<object-name>D3</object-name>
</effect>
<animation>
  <object-name>frontwheel</object-name>
  <type>rotate</type>
  <property>controls/flight/aileron</property>
  <factor>-4</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m> -1.52942 </x-m>
   <y-m> 0 </y-m>
   <z-m> 0.34695 </z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 <animation>
  <type>spin</type>
  <object-name>frontwheel</object-name>
  <property>gear/gear[0]/rollspeed-ms</property>
  <factor>10</factor>
  <center>
   <x-m> -1.52942 </x-m>
   <y-m> 0 </y-m>
   <z-m> 0.34695 </z-m>
  </center>
  <axis>
	<x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
<animation>
  <type>spin</type>
  <object-name>backwheel</object-name>
  <property>gear/gear[2]/rollspeed-ms</property>
  <factor>5</factor>
  <center>
   <x-m> 1.51073 </x-m>
   <y-m> 0 </y-m>
   <z-m> 0.34695 </z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
<animation>
  <object-name>Steering</object-name>
  <type>rotate</type>
  <property>controls/flight/aileron</property>
<factor>80</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m> -0.51353 </x-m>
   <y-m> -0.4066 </y-m>
   <z-m> 0.86443 </z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
<animation>
 <type>material</type>
 <object-name>Brakes</object-name>
<emission>
  <red>4.0</red>
  <green>0.0</green>
  <blue>0.0</blue>
  <factor-prop>/controls/gear/brake-parking</factor-prop>
 </emission>
</animation>
<animation>
<type>pick</type>
<object-name>D2</object-name>
<visible>true</visible>
<action>
<button>0</button>
<repeatable>false</repeatable>
<binding>
<command>nasal</command>
<script>aucafly.door.toggle();</script>
</binding>
</action>
<hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>door</tooltip-id>
                <label>Door Open/Close</label>
            </binding>
        </hovered>

</animation>

<animation>
<type>rotate</type>
<object-name>D1</object-name>
<object-name>D2</object-name>
<object-name>D3</object-name>
<property>sim/multiplay/generic/float[0]</property>
<factor>-40</factor>
<center>
<x-m>-1.05127</x-m>
<y-m>-0.78751</y-m>
<z-m>0.70759</z-m>
</center>
<axis>
<x>0</x>
<y>0</y>
<z>1</z>
</axis>
</animation>
